{
    "version": 2,
    "builds": [
        {
            "src": "manage.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11",
                "buildCommand": "python -m pip --version && pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "manage.py"
        }
    ]
}