{
    "version": 2,
    "builds": [
        {
            "src": "pavel_sites/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "pavel_sites/wsgi.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.11.0",
        "DJANGO_SETTINGS_MODULE": "pavel_sites.settings"
    }
}
