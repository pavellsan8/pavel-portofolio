{
    "builds": [
        {
            "src": "pavel_sites/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "pavel_sites/wsgi.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.11"
    }
}