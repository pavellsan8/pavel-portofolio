{
    "version": 2,
    "builds": [
        {
            "src": "vercel-build.sh",
            "use": "@vercel/python"
        },
        {
            "src": "pavel_sites/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "pavel_sites/wsgi.py"
        }
    ]
}